{% for enum in enums %}
public enum {{ enum.name }} {
{%- for member in enum.members %}
    {{ member.name | screamingSnakeCase }}({{ member.value }}){% unless forloop.last %},{% else %};{% endunless %}
{%- endfor %}

    private final int value;

    {{ enum.name }}(int value) {
        this.value = value;
    }

    public int getValue() {
        return value;
    }
}
{% endfor %}