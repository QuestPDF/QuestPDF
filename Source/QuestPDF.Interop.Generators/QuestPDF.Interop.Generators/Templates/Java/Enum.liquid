{% for enum in Enums %}
public enum {{ enum.Name }} {
{%- for member in enum.Members %}
    {{ member.Name }}({{ member.Value }}){% unless forloop.last %},{% else %};{% endunless %}
{%- endfor %}

    private final int value;

    {{ enum.Name }}(int value) {
        this.value = value;
    }

    public int getValue() {
        return value;
    }
}
{% endfor %}